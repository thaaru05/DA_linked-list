#include<stdio.h>
#include<Stdlib.h>

int a[15];
int tos=-1;

void push(int v)
{
	tos++;
	a[tos]=v;
}
void pop()
{
	value=a[tos];
	printf("%d",value);
	tos--;
}

void main()
{
	char exp[15]="532*+";
	int index;
	for(index=0,exp[index]!='',index++)
	push(result);
	{
		if(exp[index]=='*')
		{
			a=pop();
			b=pop();
			result=a*b;
		}
		else if(exp[index]=='/')
		{
			a=pop();
			b=pop();
			result=a/b;
		}
		else if(exp[index]=='+')
		{
			a=pop();
			b=pop();
			result=a+b;
		}
		else if(exp[index]=='-')
		{
			a=pop();
			b=pop();
			result=a-b;
		}
		else
		{
			printf("INVALID")
		}
	}
}
